[peloton]
# The repository of peloton source code
PELOTON_SRC=~/peloton
# The directory to put all the built binaries, you should configure peloton like
# ../configure --prefix=~/ it will install peloton to ~/bin ~/lib and ~/share
PELOTON_BIN=~/
# Branch of the peloton source code
PELOTON_BRANCH=master
# Host of the peloton server
PELOTON_HOST=localhost
# User to login to the peloton server. The benchmark script will execute remote 
# command like `ssh PELOTON_USER@PELOTON_HOST bash -l -c "<CMD>"'
PELOTON_USER=user
